{
  "name": "chrispenny/silverstripe-downloadable-dataobject-stash",
  "description": "Download and restore YAML fixtures for DataObjects",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "fixture", "fixtures", "yml", "dataobject", "backup", "snapshot"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Christopher David Penny",
      "email": "chris.penny@gmail.com"
    },
    {
      "name": "Sabina Talipova"
    },
    {
      "name": "Scott Sutherland"
    }
  ],
  "support": {
    "issues": "https://github.com/chrispenny/silverstripe-downloadable-dataobject-stash/issues"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "silverstripe/vendor-plugin": "^1.0",
    "silverstripe/framework": "^4.8",
    "silverstripe/cms": "^4.8",
    "dnadesign/silverstripe-populate": "^2.1",
    "chrispenny/silverstripe-data-object-to-fixture": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "slevomat/coding-standard": "^8"
  },
  "autoload": {
    "psr-4": {
      "ChrisPenny\\DataObjectStash\\": "src/",
      "ChrisPenny\\DataObjectStash\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true,
      "silverstripe/recipe-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
